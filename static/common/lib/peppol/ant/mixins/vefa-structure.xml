<?xml version="1.0" encoding="UTF-8"?>
<project name="vefa-structure">

    <macrodef name="structure-build">
        <sequential>
            <java 
                classname="no.difi.vefa.structure.Main"
                fork="true"
                failonerror="true">

                <classpath>
                    <fileset dir="${env.PDK_PATH}/lib/vefa-structure/lib" includes="*.jar"/>
                </classpath>
            </java>
        </sequential>
    </macrodef>

</project>